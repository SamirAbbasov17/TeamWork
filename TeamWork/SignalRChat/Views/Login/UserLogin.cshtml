@{
    Layout = null;
}
@model LoginModel
<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from dreamschat.dreamguystech.com/template-html/template2/login-email.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 23 Jun 2023 05:19:58 GMT -->
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Dreams Chat - Html Template</title>

    <link rel="icon" href="~/img/favicon.png">

    <link rel="stylesheet" href="~/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/plugins/fontawesome/css/all.min.css">

    <link rel="stylesheet" href="~/plugins/mcustomscroll/jquery.mCustomScrollbar.css">

    <link rel="stylesheet" href="~/css/app.css">
</head>
<body class="account-page">

    <div class="main-wrapper">

        <div class="content align-items-center">
            <div class="w-100 ">
                <div class="login-left">

                    <div class="account-content">
                        <div class="login-header">
                            <a href="index.html">
                                <img src="~/img/logo-full.png" alt>
                            </a>
                        </div>
                        <div class="form-col">
                            <div class="login-text-details">
                                <h3>Log in</h3>
                                <p>Login with your Data that you entered during your Registration</p>
                            </div>
                            <form asp-action="UserLogin" asp-controller="Login" method="post">
                                <div class="form-group">
                                    <label>Your Email</label>
                                    <input class="form-control form-control-lg group_formcontrol" name="Email" type="email">
                                </div>
                                <div class="form-group">
                                    <label>Password</label>
                                    <input class="form-control form-control-lg group_formcontrol" name="Password" type="text">
                                </div>
                                <div class="form-group">
                                    <label class="custom-check d-flex">
                                        <input id ="KeepLogeIn" name="KeepLogedIn" type="checkbox" value="true">Keep me logged in
                                        <input name="KeepLogedIn" type="hidden" value="false">Keep me logged in
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                                <div class="pt-1">
                                    <div class="text-center">
                                        <button class="btn newgroup_create btn-block d-block w-100" type="submit">Login</button>
                                    </div>
                                </div>
                                <div class="text-center dont-have">Don’t have an account? <a href="/Login/Register">Signup</a></div>
                                <div class="text-center mt-3">
                                    <span class="forgot-link">
                                        <a href="forgotpassword-email.html" class="text-end">Forgot Password ?</a>
                                    </span>
                                </div>
                            </form>
                        </div>
                        <div class="back-btn-col text-center">
                        </div>
                    </div>

                </div>
                <div class="login-right">
                </div>
            </div>
        </div>

    </div>


    <script src="~/js/jquery-3.6.0.min.js"></script>

    <script src="~/js/bootstrap.bundle.min.js"></script>

    <script src="~/js/jquery.nicescroll.min.js"></script>
    <script src="~/plugins/mcustomscroll/jquery.mCustomScrollbar.js"></script>

    <script src="~/js/script.js"></script>
</body>

<!-- Mirrored from dreamschat.dreamguystech.com/template-html/template2/login-email.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 23 Jun 2023 05:19:59 GMT -->
</html>